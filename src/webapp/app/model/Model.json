  "formulas": [
    {
      "id": 0,
      "text": "Скорость",
      "formula": "ʋ = S / t",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 1,
      "text": "Путь",
      "formula": "S = ʋ*t",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 3,
      "text": "Время",
      "formula": "t = S / ʋ",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 4,
      "text": "Средняя скорость",
      "formula": "ʋср = (S1 + S2 + …+Sn ) / (t1 + t2 + …+tn )",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 5,
      "text": "Сила тяжести",
      "formula": "FТ = m*g",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 6,
      "text": "Вес",
      "formula": "Р = m*g",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 7,
      "text": "Масса",
      "formula": "m = Р / g",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 8,
      "text": "Плотность",
      "formula": "ρ = m / V",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 9,
      "text": "Объем",
      "formula": "V=m/ρ",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 10,
      "text": "Момент силы",
      "formula": "М = F*l",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 11,
      "text": "Условие равновесия рычага",
      "formula": "a) F1 / F2 = l1 / l2 б) F1*l1 = F2*l2",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 12,
      "text": "Давление",
      "formula": "p = F / S",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 13,
      "text": "Сила давления",
      "formula": "F = р*S",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 14,
      "text": "Давление однородной жидкости",
      "formula": "p = g*ρ*h",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 15,
      "text": "Закон Архимеда",
      "formula": "FВ = ρ*g*Vт",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 16,
      "text": "Закон гидравлической машины",
      "formula": "F1 / F2 = S1 / S2",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 17,
      "text": "Механическая работа",
      "formula": "А = F*S",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 18,
      "text": "Коэффициент полезного действия механизма (КПД)",
      "formula": "ɳ = АП / АВ*100%",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 19,
      "text": "Потенциальная энергия",
      "formula": "ЕП = m*g*h",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 20,
      "text": "Кинетическая энергия",
      "formula": "ЕК = m*ʋ2 / 2",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 21,
      "text": "Мощность",
      "formula": "N = A / tN = F*ʋ",
      "class": "7",
      "chapter": "Механика"
    },
    {
      "id": 22,
      "text": "Перемещение",
      "formula": "АВ^2 = АС^2 + ВС^2",
      "class": "10",
      "chapter": "Механика"
    },
    {
      "id": 23,
      "text": "Уравнение прямолинейного равномерного движения",
      "formula": "x = x0 + Vxt",
      "class": "10",
      "chapter": "Механика"
    },
    {
      "id": 24,
      "text": "Ускорение",
      "formula": "a = v + v0 /t",
      "class": "10",
      "chapter": "Механика"
    },
    {
      "id": 25,
      "text": "Уравнение скорости",
      "formula": "v = v0 + a*t",
      "class": "10",
      "chapter": "Механика"
    },
    {
      "id": 26,
      "text": "Уравнение равноускоренного прямолинейного движения",
      "formula": "x = x0 + v0t + at^2 / 2",
      "class": "10",
      "chapter": "Механика"
    },
    {
      "id": 27,
      "text": "Закон сохранения энергии (первое начало термодинамики)",
      "formula": "Qотд = Qприн",
      "class": "8",
      "chapter": "Молекулярная физика и термодинамика"
    },
    {
      "id": 28,
      "text": "Количество теплоты",
      "formula": "Q = cmΔt",
      "class": "8",
      "chapter": "Молекулярная физика и термодинамика"
    },
    {
      "id": 29,
      "text": "Количество теплоты при сгорании топлива",
      "formula": "Q = qm",
      "class": "8",
      "chapter": "Молекулярная физика и термодинамика"
    },
    {
      "id": 30,
      "text": "Количество теплоты, необходимое для плавления вещества",
      "formula": "Q = λm",
      "class": "8",
      "chapter": "Молекулярная физика и термодинамика"
    },
    {
      "id": 31,
      "text": "Количество теплоты при парообразовании и конденсации",
      "formula": "Q = Lm",
      "class": "8",
      "chapter": "Молекулярная физика и термодинамика"
    },
    {
      "id": 32,
      "text": "Относительная влажность воздуха",
      "formula": "φ = ρ / ρн*100%",
      "class": "8",
      "chapter": "Молекулярная физика и термодинамика"
    },
    {
      "id": 33,
      "text": "Коэффициент полезного действия (КПД) теплового двигателя",
      "formula": "ɳ = An / Q1  или  ɳ = (Q1 − Q2) / Q1*100%",
      "class": "8",
      "chapter": "Молекулярная физика и термодинамика"
    },
    {
      "id": 34,
      "text": "Массовое число",
      "formula": "M = Z + N",
      "class": "9",
      "chapter": "Молекулярная физика и термодинамика"
    },
    {
      "id": 35,
      "text": "Закон Ома для участка цепи",
      "formula": "I = U / R",
      "class": "8",
      "chapter": "Электродинамика"
    },
    {
      "id": 36,
      "text": "Удельное сопротивление проводника",
      "formula": "ρ = R*S / L",
      "class": "8",
      "chapter": "Электродинамика"
    },
    {
      "id": 37,
      "text": "Законы последовательного соединения проводников",
      "formula": "1) I = I1 = I2 2) R = R1 + R2 3) U = U1 + U2",
      "class": "8",
      "chapter": "Электродинамика"
    },
    {
      "id": 38,
      "text": "Законы параллельного соединения проводников",
      "formula": "1) U = U1 = U2 2) I = I1 + I2 3) 1 / R = 1 / R1 + 1 / R2",
      "class": "8",
      "chapter": "Электродинамика"
    },
    {
      "id": 39,
      "text": "Работа электрического тока",
      "formula": "1) A = U*q 2) A = U*I*t",
      "class": "8",
      "chapter": "Электродинамика"
    },
    {
      "id": 40,
      "text": "Электрическая мощность",
      "formula": "1) P = A / t 2) P = U*I 3) P = U^2 / R",
      "class": "8",
      "chapter": "Электродинамика"
    },
    {
      "id": 41,
      "text": "Закон Джоуля-Ленца",
      "formula": "Q = I^2*R*t",
      "class": "8",
      "chapter": "Электродинамика"
    },
    {
      "id": 42,
      "text": "Абсолютный показатель преломления вещества",
      "formula": "n = c / v",
      "class": "8",
      "chapter": "Оптика"
    },
    {
      "id": 43,
      "text": "Закон Снеллиуса (показатель преломления среды)",
      "formula": "sinα / sinγ = v1 / v2 = n",
      "class": "8",
      "chapter": "Оптика"
    },
    {
      "id": 44,
      "text": "Оптическая сила линзы",
      "formula": "D = 1 / F",
      "class": "8",
      "chapter": "Оптика"
    },
    {
      "id": 45,
      "text": "Масса ядра",
      "formula": "Мя = МА – Zme",
      "class": "9",
      "chapter": "Ядерная физика"
    },
    {
      "id": 46,
      "text": "Формула дефекта масс",
      "formula": "∆m = Zmp + Nmn – MЯ",
      "class": "9",
      "chapter": "Ядерная физика"
    },
    {
      "id": 47,
      "text": "Энергия связи",
      "formula": "Есвязи = ∆m*c^2",
      "class": "9",
      "chapter": "Ядерная физика"
    },
    {
      "id": 48,
      "text": "Альфа-распад",
      "formula": "M/Z*X → 4/2*α + M/Z – 4/2*Y",
      "class": "9",
      "chapter": "Ядерная физика"
    },
    {
      "id": 49,
      "text": "Формула Планка",
      "formula": "E = h*ν",
      "class": "11",
      "chapter": "Квантовая физика"
    },
    {
      "id": 50,
      "text": "Уравнение Эйнштейна",
      "formula": "h*ν = A + (m*V^2) / 2",
      "class": "11",
      "chapter": "Квантовая физика"
    },
    {
      "id": 51,
      "text": "Энергия фотона",
      "formula": "E = h*ν = ħ*ω",
      "class": "11",
      "chapter": "Квантовая физика"
    },
    {
      "id": 52,
      "text": "Масса фотона",
      "formula": "1) E = m*c^2 2) m = (h*ν) / c^2",
      "class": "11",
      "chapter": "Квантовая физика"
    },
    {
      "id": 53,
      "text": "Импульс фотона",
      "formula": "p = m*c = (h*ν) / c = h / λ",
      "class": "11",
      "chapter": "Квантовая физика"
    },
    {
      "id": 54,
      "text": "Гипотеза Де Бройля",
      "formula": "λ = h / p",
      "class": "11",
      "chapter": "Квантовая физика"
    }
  ]
